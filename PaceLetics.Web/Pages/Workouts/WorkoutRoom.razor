@page "/Workouts/workoutroom"
@using WorkoutModule.Contracts;
@using PaceLetics.Components.Components.Workout;
@inject IWorkoutProvider woProvider

<WorkoutView Workout="@_workout"></WorkoutView>

@code {

	private IWorkout _workout;

